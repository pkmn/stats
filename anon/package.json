{
  "name": "@pkmn/anon",
  "version": "0.1.0",
  "description": "Logic for anonymizing Pok√©mon Showdown battle simulator logs",
  "repository": "github:pkmn/stats",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "files": ["build"],
  "bin": {
    "anonymize": "./anonymize"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pkmn/data": "^0.6.3",
    "@pkmn/protocol": "^0.5.1"
  },
  "devDependencies": {
    "@pkmn/dex": "^0.6.3"
  },
  "scripts": {
    "lint": "eslint --cache src --ext ts",
    "fix": "eslint --fix src --ext ts",
    "compile": "tsc -p .",
    "build": "npm run compile && tsup src/index.ts",
    "test": "jest",
    "pretest": "npm run build",
    "posttest": "npm run lint"
  },
  "tsup": {
    "outDir": "build",
    "format": ["cjs", "esm"],
    "sourcemap": true,
    "dts": true
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["node_modules/", "dist/"],
    "modulePathIgnorePatterns": ["fixtures/"]
  },
  "babel": {
    "presets": [
        "@babel/preset-typescript",
        ["@babel/preset-env", {"targets": {"node": "current"}}]
    ]
  },
  "eslintConfig": {
    "extends": "@pkmn"
  }
}
